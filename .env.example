# SalesShortcut A2A Project Environment Variables
# Copy this file to .env and fill in your API keys

# Required for Gemini LLM inference
GOOGLE_API_KEY=your_google_api_key_here

# Google Maps API
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# BigQuery Configuration  
GOOGLE_APPLICATION_CREDENTIALS=.secrets/your_service_account.json
GOOGLE_CLOUD_PROJECT=your_gcp_project_id
DATASET_ID=lead_finder_data
TABLE_ID=business_leads

# Cloud Project Configuration
CLOUD_PROJECT_ID=your_project_id
CLOUD_PROJECT_REGION=us-central1

# Optional API keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
