# SDR Service Environment Variables
# Copy this file to .env and fill in your API keys

# ElevenLabs Configuration (for voice calls)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_AGENT_ID=your_agent_id_here
ELEVENLABS_PHONE_NUMBER_ID=your_phone_number_id_here

# Twilio Configuration (alternative for voice calls)
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here

# Google Search API (optional)
GOOGLE_SEARCH_API_KEY=your_google_search_api_key_here
GOOGLE_SEARCH_ENGINE_ID=your_google_search_engine_id_here

# Google OAuth Configuration (for Gmail integration)
GOOGLE_CLOUD_CLIENT_ID=your_google_oauth_client_id_here
GOOGLE_CLOUD_CLIENT_SECRET=your_google_oauth_client_secret_here

# Service Account File (for Google services)
GOOGLE_SERVICE_ACCOUNT_FILE=.secrets/your_service_account.json